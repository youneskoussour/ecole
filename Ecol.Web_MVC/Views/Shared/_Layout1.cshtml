@model  WcfServiceLibraryproxoie.Account.User
@{ 
    var resulte = ViewBag.Username;
}
<!DOCTYPE html>
<html>
<head>
    <title>@Page.Title</title>
    @RenderSection("head", required: false)


    @Styles.Render("~/Content/css")
    @Scripts.Render("~/Scripts/js/")
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="https://kit.fontawesome.com/b99e675b6e.js"></script>
</head>
<body>

  
    <div class="wrapper">
        <div class="navbar">
            <div class="navbar_left">
                <div class="logo">
                    <a href="#">Ecole FINREG</a>
                </div>
            </div>

            <div class="navbar_right">



                <div class="profile">
                    <div class="icon_wrap">
                        <img src="https://i.imgur.com/x3omKbe.png" alt="profile_pic">
                        <span class="name">@Model.FName</span>
                        <i class="fas fa-chevron-down"></i>
                    </div>
                   
                    <div class="profile_dd">
                        <ul class="profile_ul">



                            <li><a class="address" href="#"><span class="picon"><i class=""></i></span>email</a></li>
                            <li><a class="settings" href="#"><span class="picon"><i class=""></i></span>role</a></li>
                            <li><a class="logout" href="#"><span class="picon"><i class=""></i></span>statut</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>

    </div>
                        
                       
                    
                
            



    @RenderBody()
<script>
    var rseulte = @ViewBag.Name
        console.log(rseulte);
        $(".profile .icon_wrap").click(function () {
            $(this).parent().toggleClass("active");
            $(".notifications").removeClass("active");
        });



        $(".show_all .link").click(function () {
            $(".notifications").removeClass("active");
            $(".popup").show();
        });

        $(".close, .shadow").click(function () {
            $(".popup").hide();
        });
</script>
</body>
</html>
